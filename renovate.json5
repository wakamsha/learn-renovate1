{
  // enabled: false,
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:base"],
  timezone: "Asia/Tokyo",
  separateMultipleMajor: true,
  separateMinorPatch: true,
  packageRules: [
    {
      groupName: "nodeJs",
      extends: ["group:nodeJs"],
      matchDatasources: ["docker"],
      matchPackageNames: ["cimg/node"],
      versioning: "node",
    },
    {
      groupName: "Jest",
      matchPackagePatterns: ["jest"],
      enabled: false,
    },
  ],
}
